class MyHashSet {
    /**
     * Separate Chaining for handling Collisions
     */
    int capacity;
    final float LOAD_FACTOR = 0.75f;
    int size;
    Node[] nodes;

    /**
     * Initialize your data structure here.
     */
    public MyHashSet() {
        this.capacity = 1 << 3;
        this.size = 0;
        this.nodes = new Node[this.capacity];
    }

    public void add(int key) {
        int index = this.myhash(key);
        Node head = nodes[index];
        while (head != null) {
            if (head.key == key)
                return;
            head = head.next;
        }
        nodes[index] = new Node(key, nodes[index]);
        this.size += 1;

        if (1.0 * this.size / this.capacity >= LOAD_FACTOR)
            rehash();
    }

    public void remove(int key) {
        int index = this.myhash(key);
        Node dummyHead = new Node(-1, nodes[index]);

        for (Node head = dummyHead; head.next != null; head = head.next) {
            if (head.next.key == key) {
                head.next = head.next.next;
                this.size -= 1;
                break;
            }
        }
        nodes[index] = dummyHead.next;
    }

    /**
     * Returns true if this set contains the specified element
     */
    public boolean contains(int key) {
        int index = this.myhash(key);
        Node head = nodes[index];
        while (head != null) {
            if (head.key == key)
                return true;
            head = head.next;
        }
        return false;
    }

    public int myhash(int key) {
        return key % this.capacity;
    }

    public void rehash() {
        this.capacity <<= 1;
        Node[] tmp = nodes;
        nodes = new Node[this.capacity];
        this.size = 0;
        for (Node head : tmp) {
            for (Node n = head; n != null; n = n.next) {
                this.add(n.key);
            }
        }
    }

    public void displaySet() {
        for (int i = 0; i < this.capacity; i++) {
            Node head = nodes[i];
            System.out.print(i);
            while (head != null) {
                System.out.print(" --> " + head.key);
                head = head.next;
            }
            System.out.println();
        }
    }

    class Node {
        int key;
        Node next;

        Node(int key, Node next) {
            this.key = key;
            this.next = next;
        }
    }
}
